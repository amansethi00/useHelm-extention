{
  "manifest_version": 3,
  "name": "Extention Chrome",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "webNavigation"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  }
}